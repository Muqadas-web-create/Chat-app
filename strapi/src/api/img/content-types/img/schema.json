{
  "kind": "collectionType",
  "collectionName": "imgs",
  "info": {
    "singularName": "img",
    "pluralName": "imgs",
    "displayName": "img"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "img": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "img"
    }
  }
}
